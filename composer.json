{
    "name": "therobby/slim-minimal-swoole-websocket",
    "description": "Minimal Slim 4 template with swoole and example websocket with http",
    "require": {
        "php": "^8.0",
        "slim/slim": "4.*",
        "slim/psr7": "^1.6",
        "imefisto/psr-swoole-native": "^1.1",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.0",
        "openswoole/core": "22.1.2",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
