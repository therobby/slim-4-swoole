{
    "name": "therobby/slim-minimal-swoole-websocket",
    "description": "Minimal Slim 4 template with swoole and example websocket with http",
    "version": "1.0.0",
    "require": {
        "php": "^8.4",
        "ext-swoole": "^6.1",
        "slim/slim": "4.*",
        "slim/psr7": "^1.8",
        "imefisto/psr-swoole-native": "^2.0.0",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^4.0.0",
        "phpstan/phpstan": "^2.1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
